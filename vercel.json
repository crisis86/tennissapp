{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["db.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/server.js"
    }
  ],
   "crons": [
      {
        "path": "api/cron?key=sharedKey",
        "schedule": "20 * * * *"
      }
    ]
}