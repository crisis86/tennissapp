
{
  "version": 2,
 
  "routes": [
    {
      
      "src": "/api/cron",
      "dest": "/api/cron/route.js"
    },
    {
      "src": "/(.*)",
      "dest": "/home.js"
  },
  {
    "src": "/login",
    "dest": "login.js"
}
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 11 * * *"
    }
  ]
}

 